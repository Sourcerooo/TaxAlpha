<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="input\basiszins.csv" />
    <None Remove="input\Generated File November 28, 2025 - 4_29PM.xml" />
    <None Remove="input\instruments.json" />
    <None Remove="input\priceData\BIL.csv" />
    <None Remove="input\priceData\GLD.csv" />
    <None Remove="input\priceData\IEF.csv" />
    <None Remove="input\priceData\SPY.csv" />
    <None Remove="input\priceData\TLT.csv" />
    <None Remove="input\prices.csv" />
    <None Remove="input\TaxAlpha_Raw_Data_2021.xml" />
    <None Remove="input\TaxAlpha_Raw_Data_2022.xml" />
    <None Remove="input\TaxAlpha_Raw_Data_2023.xml" />
    <None Remove="input\TaxAlpha_Raw_Data_2024.xml" />
    <None Remove="input\TaxAlpha_Raw_Data_2025.xml" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="input\configuration\basiszins.csv">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="input\configuration\instruments.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="input\configuration\prices.csv">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="input\priceData\BIL.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="input\priceData\GLD.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="input\priceData\IEF.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="input\priceData\SPY.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="input\priceData\TLT.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="input\TaxAlpha_Raw_Data_2021.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="input\TaxAlpha_Raw_Data_2022.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="input\TaxAlpha_Raw_Data_2023.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="input\TaxAlpha_Raw_Data_2024.xml">
